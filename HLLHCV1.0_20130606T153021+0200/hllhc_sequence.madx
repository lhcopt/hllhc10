posMCBRDH := posD2 + l.MBRD/2 + l.MCBRD/2 + 0.40;
posMCBRDV := posMCBRDH + l.MCBRD/2 + l.MCBRD/2 + 0.40;



MBRD.4L1.B1: MBRD, angle:=-ad2.l1, K0:=-kd2.l1;
